<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="CLOUD" class="com.google.cloud.logging.logback.LoggingAppender">
        <flushLevel>WARN</flushLevel> <!-- Optional : default ERROR -->
    </appender>
    <root level="info">
        <appender-ref ref="CLOUD" />
    </root>
</configuration>
